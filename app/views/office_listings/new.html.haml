#form-container
  .large-12.columns
    =form_for([@city, @neighborhood, @office_listing], html: {id: 'listing-attributes'}, data: 'helo' ) do |f|
      =f.label :address
      =f.text_field :address
      =f.label :office_type 
      =f.select :office_type, @office_types
      =f.label :term_length
      =f.select :term_length, @term_lengths
      =f.label :availability
      =f.select :availability, @availabilities
      
      =f.label :rent
      =f.number_field :rent
      =f.label :size
      =f.number_field :size
      =f.label :no_of_offices
      =f.number_field :no_of_offices
      =f.label :no_of_conference_rooms
      =f.number_field :no_of_conference_rooms

      =f.label :details
      =f.text_area :details

      -@amenities.each do |amenity|
        =check_box_tag('amenities[]', amenity)
        =amenity
      =f.submit
    .large-12.columns
      =form_for @image, html: { id: 'listing-images', class: 'hidden', multipart: true } do |f|
        =f.label "Upload Image"
        =f.file_field :image
