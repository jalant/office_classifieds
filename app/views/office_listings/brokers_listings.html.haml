#listings-holder 
  %h1{class: "broker-listings-test"} 
    HELLO TANAY
  .row
    -if current_broker
      -@listings.each do |listing|
        .large-4.small-12.columns
          .favorite-box
            %ul.grid.cs-style-2
              %figure
                -if listing.office_listing.images.first.link_image
                  =image_tag "#{fav.office_listing.images.first.link_image}"
                -elsif listing.office_listing.images.first.image
                  =image_tag "#{fav.office_listing.images.first.image}"
                -else 
                  =image_tag "http://www.cleaningforlondon.com/Portals/0/FlexibleOfficeSpace.jpg", class: "no-image"

                %figcaption
                  %h3 Want to see this space in person?
                  %span Contact tanay@aceoffice.com for details or check out upcoming showings: 
                  / %a{:href => "#", :"data-reveal-id" => "viewings-dates-box", :remote => true, :controller => "viewings", :action => ""}
                  %a{class: "showing button", "data-reveal-id" => "viewings-dates-box", :"data-id" => fav.office_listing.id}
                    Showings 

